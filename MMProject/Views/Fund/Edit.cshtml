@model Saving;
@{
    ViewData["Title"] = "Edit";
}

<h1>แก้ไขข้อมูล</h1>

<h1 class="text-center">Edit</h1>
<form method="post" asp-controller="Fund" asp-action="Edit">
    <div class="form-group">
        <label asp-for="Date"></label>
        <input type="text" asp-for="Date" class="form-control" />
        <span asp-validation-for="Date" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Wealt"></label>
        <input type="text" asp-for="Wealt" class="form-control" />
        <span asp-validation-for="Wealt" class="text-danger"></span>

    </div>
    <button type="submit" class="btn btn-primary"> Save </button>
</form>
